{
  "ignorePatterns": ["dist", "node_modules"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-console": "off",
    "no-debugger": "off",
    "no-param-reassign": "off"
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.config.js"
      }
    }
  }
}